generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
}

enum Role {
  ENROLLEE
  ADMIN
}


model User {
  id        String   @id @default(cuid())
  email     String   @unique
  password  String
  role      Role     @default(ENROLLEE)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

}